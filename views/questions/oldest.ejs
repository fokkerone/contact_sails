<div class="parallax filter-gradient blue" data-color="blue">
  <div class="parallax-background">
    <img class="parallax-background-image" src="/img/bkgd_seaweed.png">
  </div>
  <div class= "container">
    <div class="row">
      <div class="col-md-5 hidden-xs">
        <div class="parallax-image">
          <img class="" src="/img/image_devInTub.png" style="margin-top: 20px"/>
        </div>
      </div>
      <div class="col-md-6 col-md-offset-1">
        <div class="description">
          <h2>Who is the oldest person in the address book?</h2>
          <br>
          <h5>2. The oldest Person is <%= people.name %>,
            <%if (people.gender === 'male') { %> he <% } %>
            <%if (people.gender === 'female') { %> she <% } %>
            was born <%= people.birthdatereadable() %>
          </h5>
        </div>
        <div class="buttons">
          <a class="btn btn-fill btn-neutral" href="/questions">
            <i class="fa fa-apple"></i>Zum Dashboard
          </a>

        </div>
      </div>
    </div>
  </div>
</div>
